<div class="media">
  <div class="media-left">
    <%= link_to image_tag(artist.image.url, class: 'media-object'), artist.image.url, target: '_blank' %>
  </div>
  <div class="media-body">
    <h4 class="media-heading">(<%= artist.id %>) <%= artist.name %>: <%= artist.login_id %>, <%= artist.description %></h4>
    <h4 class="media-heading"> <%= link_to 'click here to view artist page', artist_path(artist) %> </h4>
  </div>
</div>




